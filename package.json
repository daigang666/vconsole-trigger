{
  "name": "@gangdai/vconsole-trigger",
  "version": "1.1.0",
  "description": "A lightweight vConsole trigger library for production debugging with multiple activation methods",
  "keywords": [
    "vconsole",
    "debug",
    "mobile-debug",
    "console",
    "production-debug",
    "remote-debug",
    "typescript"
  ],
  "author": "Gang Dai <gang.dai@qq.com>",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "vconsole": "^3.15.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vconsole": "^3.15.1"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/daigang666/vconsole-trigger.git"
  },
  "homepage": "https://github.com/daigang666/vconsole-trigger#readme",
  "bugs": {
    "url": "https://github.com/daigang666/vconsole-trigger/issues"
  },
  "publishConfig": {
    "access": "public"
  }
}
